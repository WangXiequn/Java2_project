<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="Root" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.MagicSquarePaneController">
   <right>
      <AnchorPane fx:id="rightpane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="return0" layoutX="25.0" layoutY="448.0" mnemonicParsing="false" onAction="#returnTotheMainMenu" prefWidth="150.0" text="return to menu" />
            <Button fx:id="solve" layoutX="25.0" layoutY="567.0" mnemonicParsing="false" onAction="#solvetheAnswer" prefWidth="150.0" text="Solve" />
            <Button fx:id="check" layoutX="25.0" layoutY="627.0" mnemonicParsing="false" onAction="#checktheAnswer" prefWidth="150.0" text="Check" />
            <Button fx:id="save" layoutX="26.0" layoutY="695.0" mnemonicParsing="false" onAction="#handleSave" prefWidth="150.0" text="Save" />
            <Label fx:id="title" layoutX="50.0" layoutY="52.0" text="Magic_Square" textFill="WHITE" />
            <VBox layoutX="7.0" layoutY="116.0" prefHeight="200.0" prefWidth="192.0" spacing="20.0">
               <children>
                  <HBox layoutX="34.0" layoutY="115.0" spacing="10.0">
                     <children>
                        <Label fx:id="dimensionLabel" layoutX="34.0" layoutY="115.0" prefHeight="20.0" prefWidth="124.0" text="dimension: " textFill="WHITE" />
                     </children>
                  </HBox>
                  <Label fx:id="dimension" prefHeight="20.0" prefWidth="94.0" text="Label" textFill="WHITE" />
                  <HBox layoutX="34.0" layoutY="159.0">
                     <children>
                        <Label fx:id="MagicSumLabel" layoutX="34.0" layoutY="159.0" text="Magic Sum: " textFill="WHITE" />
                        <Label fx:id="MagicSum" layoutX="132.0" layoutY="159.0" text="Label" textFill="WHITE" />
                     </children>
                  </HBox>
                  <HBox layoutX="39.0" layoutY="296.0">
                     <children>
                        <Label fx:id="TimerTitleLabel" layoutX="39.0" layoutY="296.0" text="Time: " textFill="WHITE" />
                        <Label fx:id="timerLabel" layoutX="104.0" layoutY="296.0" prefHeight="20.0" prefWidth="82.0" text="NAN" textFill="WHITE" />
                     </children>
                  </HBox>
                  <HBox layoutX="31.0" layoutY="355.0">
                     <children>
                        <Label fx:id="SolveTimeLabel" layoutX="31.0" layoutY="355.0" prefHeight="20.0" prefWidth="85.0" text="solveTime:" textFill="WHITE" />
                     </children>
                  </HBox>
                  <Label fx:id="solveTime" prefHeight="20.0" prefWidth="99.0" text="NAN" textFill="WHITE" />
                  <Label fx:id="GenerationLabel" prefHeight="24.0" prefWidth="118.0" text="Generation: " textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="Gerneration" prefHeight="24.0" prefWidth="73.0" text="0" textFill="WHITE" />
               </children>
            </VBox>
            <Button fx:id="pause" layoutX="25.0" layoutY="504.0" mnemonicParsing="false" onAction="#setPause" prefWidth="150.0" text="Pause" />
         </children>
      </AnchorPane>
   </right>
   <center>
      <BorderPane fx:id="Container" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
